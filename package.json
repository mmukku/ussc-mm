{
  "name": "ussc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "appcache-nanny": "^1.1.0",
    "elasticlunr": "^0.9.5",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.12",
    "uswds": "^1.3.1"
  },
  "devDependencies": {
    "appcache-manifest": "^2.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build && appcache-manifest --stamp -p /static build/static/**/*.{html,css,js,png,svg} -o build/manifest.appcache",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
