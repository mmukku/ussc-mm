{
  "name": "ussc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "appcache-nanny": "^1.1.0",
    "lodash": "^4.17.4",
    "lunr": "^2.1.4",
    "react": "16.0.0",
    "react-autosuggest": "^9.3.2",
    "react-dom": "16.0.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.14",
    "scroll-to-element": "^2.0.0",
    "uswds": "^1.3.1"
  },
  "devDependencies": {
    "appcache-manifest": "^2.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build && appcache-manifest --stamp -p /static build/static/**/*.{html,css,js,png,svg} -o build/manifest.appcache",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
